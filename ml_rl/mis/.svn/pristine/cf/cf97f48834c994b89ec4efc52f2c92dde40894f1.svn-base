
  <ui:composition template="/pages/template/common/commonLayout.xhtml"
  				  xmlns="http://www.w3.org/1999/xhtml"
				  xmlns:h="http://java.sun.com/jsf/html"
				  xmlns:ui="http://java.sun.com/jsf/facelets"
				  xmlns:f="http://java.sun.com/jsf/core"
				  xmlns:p="http://primefaces.org/ui">
 	<ui:define name="content">
		<p:messages id="messages" showDetail="false" autoUpdate="true" closable="true" /> 
		
		<p:panel id="home" header="#{msg.home}" toggleOrientation="horizontal" widgetVar="panel"  styleClass="panel" closable="true" closeSpeed="500">
				
				<p:ajax event="close" oncomplete="$('#content').remove()"/> 
		
		</p:panel>
	</ui:define>
 </ui:composition>

 
